<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <title>{% block title %}Отель "Кремлевский"{% endblock %}</title>
    {% block stylesheets %}
        <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,600" rel="stylesheet" type="text/css">
        <link href="{{ asset('bundles/brainstrapbundlesfront/css/font.css') }}" type="text/css" rel="stylesheet">
        <link href="{{ asset('bundles/brainstrapbundlesfront/css/jquery-ui.css') }}" type="text/css" rel="stylesheet">
        <link href="{{ asset('bundles/brainstrapbundlesfront/css/jquery.bxslider.css') }}" type="text/css"
              rel="stylesheet">
        <link href="{{ asset('bundles/brainstrapbundlesfront/css/style.css') }}" type="text/css" rel="stylesheet">
        <link href="{{ asset('bower/components/lightbox/css/lightbox.css') }}" type="text/css" rel="stylesheet">
        <link href="{{ asset('bundles/brainstrapbundlesfront/css/responsive.css') }}" type="text/css" rel="stylesheet">
        <link href="{{ asset('bundles/brainstrapbundlesfront/css/responsive-menu.css') }}" type="text/css"
              rel="stylesheet">
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
</head>
<body>
{% block body %}{% endblock %}
{% block javascripts %}
    <script type="text/javascript" charset="utf-8"
            src="{{ asset('bundles/brainstrapbundlesfront/js/modernizr.custom.js') }}"></script>
    <script src="{{ asset('bower/components/jquery/jquery.min.js') }}"
            type="text/javascript" charset="utf-8"></script>
    <script src="{{ asset('bower/components/jquery.migrate/jquery-migrate-1.2.1.min.js') }}"
            type="text/javascript" charset="utf-8"></script>
    <script src="{{ asset('bower/components/jquery.lazyload/jquery.lazyload.min.js') }}"
            type="text/javascript" charset="utf-8"></script>
    <script type="text/javascript" charset="utf-8"
            src="{{ asset('bundles/brainstrapbundlesfront/js/jquery-ui.min.js') }}"></script>
    <script type="text/javascript" charset="utf-8"
            src="{{ asset('bundles/brainstrapbundlesfront/js/jquery.bxslider.min.js') }}"></script>
    <script type="text/javascript" charset="utf-8"
            src="{{ asset('bundles/brainstrapbundlesfront/js/classie.js') }}"></script>
    <script type="text/javascript" charset="utf-8"
            src="{{ asset('bundles/brainstrapbundlesfront/js/mlpushmenu.js') }}"></script>
    <script src="{{ asset('bower/components/lightbox/js/lightbox.min.js') }}"
            type="text/javascript"></script>
    <script type="text/javascript" charset="utf-8"
            src="{{ asset('bundles/brainstrapbundlesfront/js/script.js') }}"></script>
    <script type="text/javascript" charset="utf-8"
            src="{{ asset('common/js/bs.utils.js') }}"></script>
{% endblock %}
</body>
</html>
